# Domain Configuration
DOMAIN=v2.praboth-sandaruwan.space

# SSL/TLS Configuration
SSL_EMAIL=sandaruwanpr@outlook.com
CERTBOT_WEBROOT=/var/www/certbot

# V2Ray Configuration
V2RAY_UUID=2e50bce3-2c41-4d46-9a25-7b4d478c855a
V2RAY_UUID_FALLBACK=2e50bce3-2c41-4d46-9a25-7b4d478c855a
V2RAY_UUID_RANDOMIZE=false
V2RAY_PATH=/beelzebub
V2RAY_PORT=10000
V2RAY_HOST=20.195.41.46
V2RAY_SERVER=v2.praboth-sandaruwan.space
SOCKS_PROXY_PORT=1080

# Nginx Configuration
NGINX_HTTP_PORT=80
NGINX_HTTPS_PORT=443
NGINX_WORKER_PROCESSES=auto
NGINX_WORKER_CONNECTIONS=1024

# Security Settings
RATE_LIMIT_REQUESTS_PER_SECOND=10
RATE_LIMIT_BURST=20
CONNECTION_LIMIT_PER_IP=20

# Logging Configuration
V2RAY_LOG_LEVEL=warning
V2RAY_ACCESS_LOG=/var/log/v2ray/access.log
V2RAY_ERROR_LOG=/var/log/v2ray/error.log
NGINX_ACCESS_LOG=/var/log/nginx/access.log
NGINX_ERROR_LOG=/var/log/nginx/error.log

# DNS Configuration
DNS_SERVERS=1.1.1.1,1.0.0.1,8.8.8.8,8.8.4.4

# Performance Settings
TCP_FAST_OPEN=true
KEEPALIVE_TIMEOUT=65
SSL_SESSION_TIMEOUT=10m
PROXY_READ_TIMEOUT=86400
PROXY_SEND_TIMEOUT=86400
PROXY_CONNECT_TIMEOUT=86400

# Security Headers
HSTS_MAX_AGE=31536000
HSTS_INCLUDE_SUBDOMAINS=true
HSTS_PRELOAD=true

# SSL Configuration
SSL_PROTOCOLS=TLSv1.2 TLSv1.3
SSL_PREFER_SERVER_CIPHERS=off
SSL_SESSION_CACHE_SIZE=10m

# Gzip Configuration
GZIP_ENABLED=true
GZIP_MIN_LENGTH=1024
GZIP_COMP_LEVEL=6

# Health Check
HEALTH_CHECK_ENDPOINT=/health
HEALTH_CHECK_RESPONSE=healthy

# Fingerprinting
TLS_FINGERPRINT=chrome
